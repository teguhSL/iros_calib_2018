title = {{A New Technique for Fully Autonomous and Efficient 3D Robotics Hand/Eye Calibration}},
volume = {5},
year = {1989}
}
@article{Nubiola2013,
abstract = {The absolute accuracy of an ABB IRB 1600 industrial robot is improved using a 29-parameter calibration model, developed after extensive experimentation. The error model takes into account all possible geometric errors (25 geometric error parameters to be identified through optimization, in addition to the pose parameters for the base and tool frames and four error parameters related to the compliance in joints 2, 3, 4 and 5). The least squares optimization technique is used to find the 29 error parameters that best fit the measures acquired with a laser tracker. Contrary to most other similar works, the validation of the robot's accuracy is performed with a very large number of measures (1,000) throughout the complete robot's joint space. After calibration, the mean/maximum position errors at any of eight different measurement points on the end-effector (all offset from axis 6 by approximately 120 mm) are reduced from 0.968 mm/2.158 mm respectively, to 0.364 mm/0.696 mm. {\textcopyright} 2012 Elsevier Ltd.},
author = {Nubiola, Albert and Bonev, Ilian A.},
doi = {10.1016/j.rcim.2012.06.004},
file = {:home/teguh/Documents/Papers/Kinematic Calibration/Absolute calibration of an ABB IRB 1600 robot using a laser tracker.pdf:pdf},
isbn = {0736-5845},
issn = {07365845},
journal = {Robotics and Computer-Integrated Manufacturing},
keywords = {Accuracy,Calibration,Industrial robot,Laser tracker,Precision},
number = {1},
pages = {236--245},
title = {{Absolute calibration of an ABB IRB 1600 robot using a laser tracker}},
volume = {29},
year = {2013}
}
@article{Wei1998,
abstract = {We propose a method for self-calibration of robotic hand-mounted$\backslash$nlaser range finders by means of active motion of the robot. Through$\backslash$nrange-measuring a plane of unknown position and orientation, the$\backslash$nmounting parameters of the range finders and the coordinates of the$\backslash$nworld plane are estimated. Systematic measurement errors can also be$\backslash$ncalibrated at the same time. The approach is fully autonomous, in that$\backslash$nno initial guesses of the unknown parameters are to be provided from the$\backslash$noutside by humans for the solution of a set of nonlinear equations. In$\backslash$nfact, the initial values are all found in closed forms by the algorithm$\backslash$nitself. Sufficient conditions for a unique solution are derived in terms$\backslash$nof controlled motion sequences. Experimental results in both a simulated$\backslash$nand a real environments are reported},
